<?xml version="1.0" encoding="utf-8"?>
<conf>
  <!--应用设定-->
  <app>
    <!--应用主题-->
    <theme>Light</theme>
    <!--语言设定-->
    <lang lang="zh-cn">
    </lang>
    <!--主窗体设定-->
    <mainwin>
      <size width="default" height="default">
      </size>
      <location center="False" X="998" Y="427">
      </location>
      <hideWhenStart>True</hideWhenStart>
      <topMost>False</topMost>
      <openPage>Home</openPage>
    </mainwin>
    <!--插件设定-->
    <plugins location="{Self}\Plug-ins\plugins" cache="{Self}\Plug-ins\cache">
    </plugins>
    <!--是否自启动-->
    <startWithOS>False</startWithOS>
    <!--是否是第一次使用-->
    <firstUse>True</firstUse>
  </app>
  <!--组件设定-->
  <com>
    <!--语言文件-->
    <lang>
      <xaml lang="zh-cn" ver="v3.0.0">简体中文</xaml>
      <xaml lang="zh-cnt" ver="v3.0.0">繁體中文</xaml>
      <xaml lang="en-us" ver="v3.0.0">English(US)</xaml>
      <xaml lang="ja-jp" ver="v3.0.0">日本</xaml>
    </lang>
    <!--插件加载器-->
    <loader>
      <load ver="v1.0.0" core="v1.0.0">
      </load>
    </loader>
  </com>
</conf>